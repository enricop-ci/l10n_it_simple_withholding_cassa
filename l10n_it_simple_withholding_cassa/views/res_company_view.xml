<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit_withholding_cassa" model="ir.ui.view">
        <field name="name">res.company.form.inherit.withholding.cassa</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Ritenuta e Cassa">
                    <group>
                        <group string="Cassa Previdenziale">
                            <field name="enable_cassa_previdenziale"/>
                            <field name="cassa_account_id"/>
                        </group>
                        <group string="Ritenuta d'Acconto">
                            <field name="enable_withholding_tax"/>
                            <field name="withholding_account_id"/>
                        </group>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>